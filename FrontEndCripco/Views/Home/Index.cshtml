@foreach (var item in Model)
{
    <div id="@item.ArticuloId" class="content" data-scrollview="true">
        <!-- begin container -->
        <div class="container" data-animation="true" data-animation-type="fadeInDown">
            <h2 class="content-title">@item.ArticuloName</h2>
            <p class="content-desc">
            </p>
            <!-- begin row -->
            <div class="row">
                <div class="col-sm-12 col-sm-12">
                    <div class="row">
                        <div  align="center">
                            @if (item.detalleArticuloModels.Count > 0)
                            {
                                <img   src="data:image/jpg;base64,@item.detalleArticuloModels[0].Imagen" class="img-rounded" width="550" height="400" />
                            }
                        </div>
                        <br />
                        <br />
                        <div class="col-md-12 col-sm-12">
                            <p>
                                @item.Descripcion<br />
                            </p>
                            <!-- end about-author -->
                        </div>
                    </div>
                </div>
                <!-- end col-4 -->
            </div>
            <!-- end row -->
        </div>
        <!-- end container -->
    </div>


    <div id="milestone" class="content bg-black-darker has-bg" data-scrollview="true">
        <!-- begin content-bg -->
        <div class="content-bg">
            <img src="/assets/img/milestone-bg.jpg" alt="Milestone">
        </div>
        <!-- end content-bg -->
        <!-- begin container -->
        <!-- end container -->
    </div>

}
